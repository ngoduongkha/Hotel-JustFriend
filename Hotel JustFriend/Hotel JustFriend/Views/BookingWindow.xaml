        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Hotel_JustFriend.Views"
        mc:Ignorable="d"
        Title="BookingWindow" Height="300" Width="500">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Button Grid.Row="0" Content="thue phong"></Button>
        <Button Grid.Row="1" Content="don dep"></Button>
        <Button Grid.Row="2" Content="them khach hang"></Button>
        <Button Grid.Row="3" Content="them dich vu"></Button>
        <Button Grid.Row="4" Content="tra phong"></Button>
        <TextBlock x:Name="idroom" Visibility="Hidden"></TextBlock>
    </Grid>
